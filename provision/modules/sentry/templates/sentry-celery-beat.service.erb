[Unit]
Description=Celery beat worker
After=network.target multi-user.target

[Service]
User=<%= scope['sentry::params::user'] %>
Group=<%= scope['sentry::params::group'] %>
Environment=SENTRY_CONF=/etc/sentry/
WorkingDirectory=/opt/sentry

ExecStart=/usr/bin/sentry celery beat
ExecReload=/bin/kill -HUP $MAINPID

